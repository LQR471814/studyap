{
  "name": "studyap",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:ui": "dotenv -- vite",
    "dev:api": "dotenv -- wrangler dev",
    "build": "dotenv -- vite build",
    "preview": "vite preview",
    "lint": "tsc --noEmit && svelte-check --tsconfig ./tsconfig.json",
    "test": "jest",
    "script:generate": "dotenv -- tsx generation/cli/generate.ts",
    "script:list-tests": "dotenv -- tsx generation/cli/list-tests.ts",
    "script:view-tests": "dotenv -- tsx generation/cli/view-tests.ts",
    "db:generate": "drizzle-kit generate:sqlite && drizzle-rel-gen -snapshot=\"drizzle/meta/0000_snapshot.json\" -src=\"lib/schema/schema.ts\" -module=\"./schema\" -outfile=\"lib/schema/schema.relations.ts\"",
    "db:push": "drizzle-kit push:sqlite"
  },
  "dependencies": {
    "@libsql/client": "^0.6.0",
    "@trpc/client": "11.0.0-rc.382",
