{
  "name": "studyap",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "tsc --noEmit",
    "test": "jest",
    "script:generate": "dotenv -- tsx generation/cli/generate.ts",
    "script:list-tests": "dotenv -- tsx generation/cli/list-tests.ts",
    "script:view-tests": "dotenv -- tsx generation/cli/view-tests.ts",
    "db:generate": "drizzle-kit generate:sqlite && drizzle-rel-gen -snapshot=\"drizzle/meta/0000_snapshot.json\" -src=\"schema/tests.ts\" -module=\"./tests\" -outfile=\"schema/tests.relations.ts\"",
    "db:push": "drizzle-kit push:sqlite"
  },
  "dependencies": {
    "@libsql/client": "^0.6.0",
    "@tailwindcss/typography": "^0.5.10",
    "drizzle-orm": "^0.30.8",
    "jsflags": "^1.3.0",
    "openai": "^4.24.1",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "autoprefixer": "^10.0.1",
    "dotenv-cli": "^7.4.1",
    "drizzle-kit": "^0.20.14",
    "drizzle-relations-generator": "^0.0.1",
    "jest": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.7.2",
    "typescript": "^5"
  }
}
