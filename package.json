{
  "name": "studyap",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:ui": "dotenv -e .dev.vars -- vite",
    "dev:api": "wrangler dev",
    "dev:gen": "dotenv -e .dev.vars -- pnpm tsx cmd/generator/apush-dev",
    "dev": "mprocs",
    "build": "dotenv -e .dev.vars -- vite build",
    "deploy:api": "wrangler deploy",
    "preview": "vite preview",
    "lint:tsc": "tsc --noEmit",
    "lint:svelte": "svelte-check --tsconfig ./tsconfig.json",
    "lint:biome": "biome check --write --unsafe .",
    "lint:pkgs": "syncpack set-semver-ranges && syncpack fix-mismatches",
    "lint": "pnpm lint:tsc && pnpm lint:svelte && pnpm lint:biome",
    "test": "dotenv -e .dev.vars -- vitest run",
    "tsx": "dotenv -e .dev.vars -- tsx",
    "db:generate:main": "drizzle-kit generate && drizzle-rel-gen -snapshot=\"drizzle/meta/0000_snapshot.json\" -src=\"lib/schema/schema.ts\" -module=\"./schema\" -outfile=\"lib/schema/schema.relations.ts\"",
    "db:generate:llm-cache": "drizzle-kit generate --config=lib/llm-cache/drizzle.config.ts && drizzle-rel-gen -snapshot=\"lib/llm-cache/drizzle/meta/0000_snapshot.json\" -src=\"lib/llm-cache/schema/schema.ts\" -module=\"./schema\" -outfile=\"lib/llm-cache/schema/schema.relations.ts\"",
    "db:generate": "pnpm db:generate:main && pnpm db:generate:llm-cache",
    "db:push": "dotenv -e .dev.vars -- drizzle-kit push",
    "db:view": "dotenv -e .dev.vars -- drizzle-kit studio",
    "db:reset": "docker compose down -v && docker compose up -d && pnpm db:push"
  },
  "dependencies": {
    "@google/generative-ai": "0.12.0",
    "@libsql/client": "0.6.2",
    "@microlabs/otel-cf-workers": "1.0.0-rc.40",
    "@opentelemetry/api": "1.6.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.52.0",
    "@opentelemetry/resources": "^1.25.0",
    "@opentelemetry/sdk-trace-base": "^1.25.0",
    "@opentelemetry/sdk-trace-node": "^1.25.0",
    "@opentelemetry/semantic-conventions": "^1.25.0",
    "@tanstack/svelte-query": "^5.45.0",
    "@trpc/client": "11.0.0-rc.382",
    "@trpc/server": "11.0.0-rc.382",
    "better-sqlite3": "11.0.0",
    "bits-ui": "0.21.10",
    "clsx": "2.1.1",
    "date-fns": "3.6.0",
    "drizzle-orm": "0.31.2",
    "es-main": "1.3.0",
    "jsflags": "1.3.0",
    "lucide-svelte": "0.394.0",
    "openai": "4.50.0",
    "p-debounce": "4.0.0",
    "p-queue": "8.0.1",
    "remove": "0.1.5",
    "seedrandom": "3.0.5",
    "superjson": "2.2.1",
    "svelte-markdown": "0.4.1",
    "svelte-multiselect": "10.3.0",
    "svelte-spa-router": "4.0.1",
    "tailwind-merge": "2.3.0",
    "tailwind-variants": "0.2.1",
    "testcontainers": "10.9.0",
    "zod": "3.23.8",
    "zod-to-gemini-schema": "0.1.1",
    "zod-to-json-schema": "3.23.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.1",
    "@cloudflare/workers-types": "4.20240605.0",
    "@iconify-json/ri": "1.1.20",
    "@sveltejs/vite-plugin-svelte": "3.1.1",
    "@tsconfig/svelte": "5.0.4",
    "@types/better-sqlite3": "7.6.10",
    "@types/node": "20.14.2",
    "@types/seedrandom": "3.0.8",
    "autoprefixer": "10.4.19",
    "dotenv-cli": "7.4.2",
    "drizzle-kit": "0.22.7",
    "drizzle-relations-generator": "0.0.1",
    "mprocs": "0.6.4",
    "postcss": "8.4.38",
    "svelte": "4.2.18",
    "svelte-check": "3.8.0",
    "svelte-headless-table": "0.18.2",
    "syncpack": "12.3.2",
    "tailwindcss": "3.4.4",
    "tslib": "2.6.3",
    "tsx": "4.15.2",
    "typescript": "5.4.5",
    "unplugin-icons": "0.19.0",
    "vite": "5.2.13",
    "vitest": "1.6.0",
    "wrangler": "3.60.1"
  },
  "packageManager": "pnpm@9.3.0+sha512.ee7b93e0c2bd11409c6424f92b866f31d3ea1bef5fbe47d3c7500cdc3c9668833d2e55681ad66df5b640c61fa9dc25d546efa54d76d7f8bf54b13614ac293631"
}
