{
  "name": "studyap",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:ui": "dotenv -- vite",
    "dev:api": "dotenv -- wrangler dev",
    "build": "dotenv -- vite build",
    "preview": "vite preview",
    "lint": "tsc --noEmit && svelte-check --tsconfig ./tsconfig.json",
    "test": "jest",
    "script:generate": "dotenv -- tsx generation/cli/generate.ts",
    "script:list-tests": "dotenv -- tsx generation/cli/list-tests.ts",
    "script:view-tests": "dotenv -- tsx generation/cli/view-tests.ts",
    "db:generate": "drizzle-kit generate:sqlite && drizzle-rel-gen -snapshot=\"drizzle/meta/0000_snapshot.json\" -src=\"schema/tests.ts\" -module=\"./tests\" -outfile=\"schema/tests.relations.ts\"",
    "db:push": "drizzle-kit push:sqlite"
  },
  "dependencies": {
    "@libsql/client": "^0.6.0",
    "bits-ui": "^0.21.3",
    "drizzle-orm": "^0.30.8",
    "itty-router": "^5.0.12",
    "jsflags": "^1.3.0",
    "openai": "^4.24.1",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240405.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@swc/jest": "^0.2.36",
    "@tsconfig/svelte": "^5.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/route-parser": "^0.1.7",
    "autoprefixer": "^10.0.1",
    "dotenv-cli": "^7.4.1",
    "drizzle-kit": "^0.20.14",
    "drizzle-relations-generator": "^0.0.1",
    "jest": "^29.7.0",
    "postcss": "^8",
    "svelte": "^4.2.14",
    "svelte-check": "^3.6.9",
    "tailwindcss": "^3.3.0",
    "tslib": "^2.6.2",
    "tsx": "^4.7.2",
    "typescript": "^5",
    "vite": "^5.2.8",
    "vite-tsconfig-paths": "^4.3.2",
    "wrangler": "^3.50.0"
  }
}
